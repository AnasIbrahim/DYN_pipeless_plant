                <Ellipse Margin="0,0,275,0" Height="15" Fill="{StaticResource LGBRed}" Name="agv5LED" Stroke="Black" Width="15" HorizontalAlignment="Right" VerticalAlignment="Center"/>
                <TextBlock HorizontalAlignment="Right" VerticalAlignment="Center" Text="Automatic:" FontFamily="Calibri" FontSize="12" Margin="0,0,130,0"></TextBlock>
