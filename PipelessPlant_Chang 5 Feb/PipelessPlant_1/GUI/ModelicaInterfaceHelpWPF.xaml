        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.Background>
            <ImageBrush ImageSource="/MULTIFORM_PCS;component/GUI/Images/ModelicaInterfacePP.png"></ImageBrush>
        </Grid.Background>
        <Label Content="double[] digitalInputs" Grid.Row="1" Height="28" HorizontalAlignment="Left" Margin="174,12,0,0" Name="label1" VerticalAlignment="Top" />
