<UserControl x:Class="MULTIFORM_PCS.GUI.UserControlsView.AGV"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="33" d:DesignWidth="33" Name="agvControl">
    <Grid Name="mainGrid" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <Ellipse HorizontalAlignment="Stretch" Margin="0,0,0,0" Name="ellipseAGV" Stroke="Black" VerticalAlignment="Stretch" StrokeThickness="1">
            <Ellipse.Fill>
                <ImageBrush ImageSource="/MULTIFORM_PCS;component/GUI/Images/iRobotCreate.png"></ImageBrush>
            </Ellipse.Fill>
        </Ellipse>
        <Rectangle Name="spot"  Height="15" Margin="11.5,9,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="10" Stroke="Gray" Opacity="0.5" Fill="White" StrokeThickness="1"></Rectangle>
    </Grid>
</UserControl>
